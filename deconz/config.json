{
  "name": "deCONZ",
  "version": "latest-beta",
  "slug": "deconz-latest",
  "description": "deCONZ",
  "startup": "services",
  "boot": "auto",
  "hassio_api": false,
  "homeassistant_api": false,
  "host_network": true,
  "webui": "http://[HOST]:[PORT:8234]/pwa/login.html",
  "auto_uart":true,
  "map": [
    "config:rw"
  ],
  "options": {
    "httpport": 8234,
    "wsport": 8235,
    "conbee": true,
      "xpra": false,
      "debug_info?": 1,
      "debug_aps?": 0,
      "debug_zcl?": 0,
      "debug_zdp?": 0,
      "debug_otau?": 0
  },
  "schema": {
    "httpport": "port",
    "wsport": "port",
    "conbee": "bool",
    "xpra": "bool"
  },
  "ports": {
    "8234/tcp": 8234,
    "8235/tcp": 8235,
    "8236/tcp": 8236
  }
}
